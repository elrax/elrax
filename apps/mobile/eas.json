{
	"cli": {
		"version": ">= 5.4.0"
	},
	"build": {
		"base": {
			"bun": "1.0.25",
			"android": {
				"resourceClass": "medium"
			},
			"ios": {
				"resourceClass": "m-medium"
			}
		},
		"simulator": {
			"extends": "base",
			"channel": "development",
			"developmentClient": true,
			"distribution": "internal",
			"ios": {
				"simulator": true
			}
		},
		"development": {
			"extends": "base",
			"channel": "development",
			"developmentClient": true,
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			}
		},
		"staging": {
			"extends": "base",
			"channel": "staging",
			"distribution": "internal"
		},
		"production": {
			"extends": "base",
			"channel": "production",
			"distribution": "store",
			"android": {
				"buildType": "app-bundle"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
